<p id="notice"><%= notice %></p>
<p>
  <strong>Descrição:</strong>
  <%= @questionario.nome %>
</p>
<p>
  <strong>Vigência início:</strong>
  <%= @questionario.vigencia_inicio.strftime("%d/%m/%Y") %>
</p>
<p>
  <strong>Vigência fim:</strong>
  <%= @questionario.vigencia_fim.strftime("%d/%m/%Y") %>
</p>

<div class="btn btn-info"><%= link_to 'Editar', edit_questionario_path(@questionario) %></div>
<div class="btn btn-danger"><%= link_to 'Voltar', questionarios_path %></div>